<tryton>
    <data>
        <record model="ir.message" id="msg_delete_document_cash_bank">
            <field name="text">Document %(doc_name)s "%(doc_number)s" must be in "%(state)s" state for deletion.</field>
        </record>
        <record model="ir.message" id="msg_receipt_no_lines">
            <field name="text">Receipt %(receipt)s has no lines.</field>
        </record>
        <record model="ir.message" id="msg_diff_total_lines_cash_bank">
            <field name="text">Difference between "Total" and "Total Lines" must be zero.</field>
        </record>
        <record model="ir.message" id="msg_party_required_cash_bank">
            <field name="text">Party is required.</field>
        </record>
        <record model="ir.message" id="msg_total_less_zero">
            <field name="text">Total is less than zero.</field>
        </record>
        <record model="ir.message" id="msg_invalid_amount_sign">
            <field name="text">Amount "%(amount)s" has a different sign than document "%(document)s" amount.</field>
        </record>
        <record model="ir.message" id="msg_amount_greater_amount_to_apply">
            <field name="text">Amount "%(amount)s" is greater than document "%(document)s" avaiable amount to apply.</field>
        </record>
    </data>
</tryton>
